{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "Smit Desai",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test": "jest",
    "analyze": "nuxt-ts build --analyze"
