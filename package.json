    "bootstrap": "^4.4.1",
    "nuxt": "^2.0.0",
    "vue-lazyload": "^1.3.3",
    "vue-property-decorator": "^8.4.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "stylelint": "^10.1.0",
